{
  "name": "@reactjsf/as-table",
  "version": "0.1.5",
  "description": "reactjsf table components",
  "scripts": {
    "build": "npm run build:tsc",
    "build:tsc": "tsc -d",
    "prebuild": "npm run prebuild:clean && npm run prebuild:version",
    "prebuild:clean": "rimraf es",
    "prebuild:version": "npm  --no-git-tag-version version patch",
    "postbuild": "npm run postbuild:copy",
    "postbuild:copy": "copy-files-from-to"
  },
  "copyFiles": [
    {
      "from": "package.json",
      "to": "es/package.json"
    },
    {
      "from": "README.md",
      "to": "es/README.md"
    }
  ],
  "main": "lib/index.js",
  "engineStrict": false,
  "engines": {
    "npm": ">=2.14.7",
    "node": ">=6"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@reactjsf/core": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-async-to-generator": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.9.23",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.11.0",
    "copy-files-from-to": "^2.3.0",
    "eslint": "^5.14.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.4.0",
    "react": "^16.8.3",
    "typescript": "^3.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrtemel/reactjsf-as-table.git"
  },
  "author": "Haydar RÄ±dvan TEMEL <hrtemel@gmail.com>",
  "keywords": [
    "react",
    "form",
    "json-schema"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hrtemel/reactjsf-as-table#readme"
}
